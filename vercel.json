{
  "rewrites": [
    {
      "source": "/",
      "destination": "/src/index.html"
    },
    {
      "source": "/poster-design",
      "destination": "/src/poster_design.html"
    },
    {
      "source": "/video-editing",
      "destination": "/src/video_editing.html"
    },
    {
      "source": "/monetization-engine",
      "destination": "/src/monetization_engine.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}